{
  "name": "@poly/web",
  "version": "1.0.0",
  "description": "Poly React web app",
  "main": "index.tsx",
  "scripts": {
    "clean": "rimraf dist/*",
    "dev": "npm run prebuild && wp --env dev",
    "copy": "copyfiles -u 1 ./src/index.ejs ./src/icons/* ./src/favicon.ico ./dist",
    "build": "npm run copy && cross-env NODE_ENV=production wp --env prod",
    "prebuild": "npm run clean",
    "format": "npm run format:base \"**/*.{ts,tsx,js,jsx,yml,json,md}\"",
    "format:base": "npx prettier",
    "format:fix": "npm run format -- --write",
    "lint": "npm run lint:base \"**/*.{ts,tsx,js,jsx}\"",
    "lint:base": "npm run tsc && npx eslint",
    "lint:fix": "npm run lint --fix",
    "tsc": "tsc",
    "test:nocoverage": "jest --coverage=false",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --coverage=false",
    "test-watch": "jest --config jest.config.js --watch --coverage=false",
    "storybook": "start-storybook -p 6006"
  },
  "keywords": [
    "react",
    "webpack",
    "redux"
  ],
  "author": "Nitin Panwar",
  "dependencies": {
    "@sentry/browser": "^7.14.1",
    "@storybook/addon-storyshots": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "assert": "^2.0.0",
    "axios": "^1.0.0",
    "change-case": "^4.1.2",
    "chroma-js": "^2.4.2",
    "ejs": "^3.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "statsig-react": "^1.17.0",
    "styled-components": "^5.3.6",
    "express": "~4.18.2",
    "helmet": "~6.0.1",
    "winston": "~3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-do-expressions": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-bind": "^7.18.9",
